{
  "name": "RedWillow.MvcToastrFlash",
  "description": "This package adds Rails-like flash functionality to your MVC controllers, backed by the excellent Toastr JavaScript notification library. Flash notifications are persisted through redirects and are removed when shown.",
  "copyright": "Copyright (c) 2016 Red Willow",
  "title": "MVC Toastr Flash",
  "authors": [ "Jason Kelly" ],
  "version": "1.1.0-*",
  "packOptions": {
    "owners": [ "Red Willow" ],
    "summary": "Add Rails-like flash functionality to your MVC controllers, backed by the excellent Toastr JavaScript notification library.",
    "licenseUrl": "https://github.com/red-willow/mvc-toastr-flash#license",
    "iconUrl": "https://red-willow.github.io/mvc-toastr-flash/images/rw64.png",
    "tags": [ "asp.net", "mvc", "asp", "toast", "toastr", "flash", "asp.net 5" ],
    "projectUrl": "https://github.com/red-willow/mvc-toastr-flash",
    "releaseNotes": ""
  },
  "configurations": {
    "Debug": {
      "buildOptions": {
        "define": [ "DEBUG" ],
        "optimize": false
      }
    },
    "Release": {
      "buildOptions": {
        "allowUnsafe": false,
        "optimize": true
      }
    }
  },
  "frameworks": {
    "net45": {
      "dependencies": {
        "Microsoft.AspNet.Mvc": "3.0.20105.1",
        "Microsoft.AspNet.Razor": "1.0.20105.408",
        "Microsoft.AspNet.WebPages": "1.0.20105.408",
        "Microsoft.Web.Infrastructure": "1.0.0.0"
      },
      "buildOptions": {
        "define": [ "FULLBUILD" ]

      }
    },
    "netstandard1.6": {
      "imports": [
        "dotnet",
        "portable-net451+win8"
      ],
      "dependencies": {
        "Microsoft.AspNetCore.Html.Abstractions": "1.0.0",
        "Microsoft.AspNetCore.Mvc.Core": "1.1.0",
        "Microsoft.AspNetCore.Mvc.ViewFeatures": "1.0.1",
        "NETStandard.Library": "1.6.1"
      },
      "buildOptions": {
        "define": [ "COREBUILD" ]

      }
    }
  },
  "buildOptions": {
    "optimize": true
  }
}
